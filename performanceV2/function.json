{
    "bindings": [
        {
            "authLevel": "function",
            "type": "httpTrigger",
            "direction": "in",
            "name": "req",
            "methods": [
                "post",
                "get",
                "delete",
                "put"
            ],
            "route": "performanceV2/{action?}"
        },
        {
            "type": "http",
            "direction": "out",
            "name": "$return"
        }
    ]
}